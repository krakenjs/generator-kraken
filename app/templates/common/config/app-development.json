{

    "express": {
        "view engine": "dust",
        "view cache": false,
        "views": "path:./public/templates"
    },

    "view engines": {
        "dust": {
            "module": "engine-munger",
            "renderer": {
                "method": "dust",
                "arguments": [{ "cache": false }]
            }
        }
    }

}